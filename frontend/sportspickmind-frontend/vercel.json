{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}
